Relationship ecommerce_orders_dim_dates {
  type: "many_to_one"
  from: FieldRef {
    model: 'ecommerce_orders'
    field: 'created_date'
  }
  to: FieldRef {
    model: 'dim_dates'
    field: 'date_key'
  }
}

Relationship ecommerce_orders_order_master {
  type: "one_to_one"
  from: FieldRef {
    model: 'ecommerce_orders'
    field: 'user_id'
  }
  to: FieldRef {
    model: 'order_master'
    field: 'user_id'
  }
}

